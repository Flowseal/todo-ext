{
    "manifest_version": 2,
    "name": "Rawr",
    "description": "aboba",
    "version": "1.0",
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "./svgs/icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage"
    ],
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}